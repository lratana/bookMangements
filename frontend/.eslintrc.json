{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "@vue/eslint-config-prettier",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-debugger": "warn",
        "vue/no-unused-vars": "warn",
        "prettier/prettier": [
            {
                "singleQuote": true,
                "parser": "flow"
            }
        ]
    }
}